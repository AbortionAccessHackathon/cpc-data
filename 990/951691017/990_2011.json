75887", "ContributionsGrantsCurrentYear": "160438", "ProgramServiceRevenuePriorYear": "9276954", "ProgramServiceRevenueCY": "9214160", "InvestmentIncomePriorYear": "15072", "InvestmentIncomeCurrentYear": "6410", "OtherRevenuePriorYear": "0", "OtherRevenueCurrentYear": "0", "TotalRevenuePriorYear": "9467913", "TotalRevenueCurrentYear": "9381008", "GrantsAndSimilarAmntsPriorYear": "0", "GrantsAndSimilarAmntsCY": "0", "BenefitsPaidToMembersPriorYear": "0", "BenefitsPaidToMembersCY": "0", "SalariesEtcPriorYear": "6879002", "SalariesEtcCurrentYear": "6708860", "TotalProfFundrsngExpPriorYear": "0", "TotalProfFundrsngExpCY": "0", "TotalFundrsngExpCurrentYear": "60158", "OtherExpensePriorYear": "2587173", "OtherExpensesCurrentYear": "2611043", "TotalExpensesPriorYear": "9466175", "TotalExpensesCurrentYear": "9319903", "RevenuesLessExpensesPriorYear": "1738", "RevenuesLessExpensesCY": "61105", "TotalAssetsBOY": "2966786", "TotalAssetsEOY": "2623508", "TotalLiabilitiesBOY": "2701436", "TotalLiabilitiesEOY": "2269488", "NetAssetsOrFundBalancesBOY": "265350", "NetAssetsOrFundBalancesEOY": "354020", "InfoInScheduleOPartIII": "X", "MissionDescription": "THE GUIDANCE CENTER PROVIDES EFFECTIVE, ACCESSIBLE, CULTURALLY RELEVANT MENTAL HEALTH SERVICES FOR CHILDREN AND FAMILIES; COORDINATES SERVICES WITH ALLIED AGENCIES, CLINICS, SCHOOLS, AND OTHERS; AND OFFERS SPECIFIC PROFESSIONAL TRAINING IN MENTAL HEALTH DISCIPLINES.", "SignificantNewProgramServices": "0", "SignificantChange": "0", "Expense": "4577834", "Revenue": "5637483", "Description": "OUTPATIENT MENTAL HEALTH CARE IS PROVIDED AT LOW COST TO CHILDREN AND FAMILIES WHO CANNOT AFFORD PRIVATE CARE. SERVICES ARE PROVIDED THROUGH SEVERAL PROGRAMS SPONSORED BY THE LOS ANGELES COUNTY DEPARTMENT OF MENTAL HEALTH (DMH) AND OTHER CONTRACT AND GRANT-BASED PROGRAMS. THE GUIDANCE CENTER SERVED OVER 2,100 CLIENTS DURING THE YEAR, AND PROVIDED OVER 100,000 HOURS OF MENTAL HEALTH SERVICES. SERVICES ARE PROVIDED AT THREE CLINIC FACILITIES. IN ADDITION, TRAINING IS PROVIDED TO MENTAL HEALTH PROFESSIONALS IN PERFORMING AND DOCUMENTING THESE SERVICES UNDER DMH STANDARDS.", "Activity2": {"Expense": "2130872", "Revenue": "2215577", "Description": "SCHOOL-BASED PROGRAM: MENTAL HEALTH SERVICES ARE OFFERED TO CHILDREN AND TEENS AT MORE THAN 20 SCHOOLS WITHIN THE LONG BEACH UNIFIED SCHOOL DISTRICT. THE PROGRAM PROVIDES ACCESS TO CHILDREN AND FAMILIES WHO ARE RETICENT TO ATTEND A TRADITIONAL MENTAL HEALTH CARE CLINIC FOR SERVICES. PROGRAM COORDINATORS WORK CLOSELY WITH SCHOOL COUNSELORS TO IDENTIFY CHILDREN WHOSE BEHAVIOR IS INTERFERING WITH THEIR ABILITY TO FUNCTION EFFECTIVELY IN SCHOOL OR AT HOME. THERAPISTS PROVIDE INDIVIDUAL, FAMILY, AND GROUP THERAPY AT THE SCHOOL SITE OR IN THE CHILD'S HOME, WORKING TO INCREASE THE CHILD'S ABILITY TO FUNCTION AT SCHOOL, IN THE HOME, AND IN THE COMMUNITY."}, "Activity3": {"Expense": "1232226", "Revenue": "1351771", "Description": "INTENSIVE SERVICES: SPECIALIZED SERVICES FOR CHILDREN WITH SERIOUS MENTAL ILLNESSES WHO ARE AT RISK OF OUT-OF-HOME PLACEMENT SUCH AS FOSTER CARE, PSYCHIATRIC HOSPITALIZATION, OR JUVENILE DETENTION. STAFF MEMBERS WORK WITH CLIENTS IN A COMMUNITY SETTING, AT HOME, IN SCHOOL, OR IN COURTS TO HELP THE CHILDREN REDUCE SYMPTOMS OF DEPRESSION, ATTENTION DEFICIT HYPERACTIVITY DISORDER, OPPOSITIONAL DEFIANCE DISORDER, PAST TRAUMA, AND TO IMPROVE THEIR QUALITY OF LIFE. STAFF ALSO HELP CLIENTS MANAGE THEIR ANGER, INCREASE COPING SKILLS, REDUCE FAMILY CONFLICT, AND INCREASE SELF-ESTEEM."}, "ActivityOther": {"Expense": "15730", "Revenue": "9329", "Description": "TRAINING SYMPOSIUM FOR MENTAL HEALTH PROFESSIONALS."}, "TotalOfOtherProgramServiceExp": "15730", "TotalOfOtherProgramServiceRev": "9329", "TotalProgramServiceExpense": "7956662", "DescribedIn501C3": {"@referenceDocumentId": "RetDoc1039100001", "#text": "1"}, "ScheduleBRequired": {"@referenceDocumentId": "RetDoc1234500001", "#text": "1"}, "PoliticalActivities": {"@referenceDocumentId": "RetDoc1039700001",